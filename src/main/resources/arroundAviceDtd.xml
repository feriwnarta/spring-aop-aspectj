<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="objek" class="com.ferdev.aop.dtd.aroundadvice.Objek"></bean>
    <bean id="arroundAdviceExample" class="com.ferdev.aop.dtd.aroundadvice.ArroundAdviceContoh"></bean>

    <bean id="proksiObjek" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="target" ref="objek"></property>
        <property name="interceptorNames">
            <list>
                <value>arroundAdviceExample</value>
            </list>
        </property>
    </bean>


</beans>